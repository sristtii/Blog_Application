{
    "builds": [
      {
        "src": "dproject/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfiles_build" }
      }
    ],
    "buildCommand": "pip install -r requirements.txt",
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles_build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "dproject/wsgi.py"
      }
    ]
  }
  